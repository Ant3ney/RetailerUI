{
	"name": "retailer-ui",
	"version": "0.2.9",
	"description": "A React UI Library for e-commerce retail dropshipping businesses.",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"dependencies": {
		"react-animate-height": "^2.1.2",
		"react-responsive": "^9.0.0-beta.8",
		"react-scripts": "5.0.0",
		"web-vitals": "^2.1.4",
		"react": "*",
		"react-dom": "*"
	},
	"peerDependencies": {
		"react": "*",
		"react-dom": "*"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "node scripts/buildApp.js && set NODE_ENV=production && babel src/jsBuild --out-dir dist --copy-files",
		"build-ts": "cd ./src/jsBuild/Nav && mkdir media && cd ../../../ && Xcopy /E /I \"./src/lib/Nav/media\" \"./src/jsBuild/Nav/media\"",
		"build-ts-old": "tsc && Copy-Item \"./src/lib/Nav/media\" -Destination \"./src/jsBuild/Nav/media\" -Recurse",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"build-test": "node scripts/buildApp.js"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/cli": "^7.17.6",
		"@babel/core": "^7.17.7",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-react": "^7.17.12",
		"@types/react": "^18.0.9"
	}
}
